{
    "manifest_version": 2,
    "name": "UN Helper",
    "version": "1.0",
    "description": "An extension to access a local API and display the data in the console.",
    "permissions": [
      "http://localhost:3000/"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "images/icon-48.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon-48.png"
    }
    }
  }
  